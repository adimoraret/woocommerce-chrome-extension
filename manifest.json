{
    "manifest_version" : 2,

    "name" : "Woo Commerce",
    "description" : "Manage your Woo Commerce website",
    "version" : "1.0",

    "browser_action" : {
        "default_icon" : "images/icon.png",
        "default_popup" : "popup.html"
    },

    "permissions": [
        "http://*.codetrest.com/",
        "https://*.codetrest.com/"
    ],

    "options_page" : "options.html",
    "options_ui": {
        "chrome_style": true,
        "page": "options.html"
    },

    "chrome_url_overrides":{
        "newtab": "index.html"
    }
}